<% unless @level_3.nil? %>
	<ul>
	<% @level_3.each do |level_3| -%>
	<% @no_route = 'No route ' + level_3.navlabel %>
			<li class="admin_navlabel">
				<% if level_3.redirect? %>
					<%#= link_to level_3.navlabel, {:controller => level_3.controller_redirect, :action => level_3.action_redirect, :id => level_3.resource_id} %>
					<%= link_to(level_3.navlabel, '/'+ level_3.controller_redirect.to_s + '/' + level_3.action_redirect.to_s) rescue link_to @no_route.html_safe %>
				<% else %>
					<%#= link_to level_3.navlabel, {:controller => level_3.controller_name} %>
					<%= link_to(level_3.navlabel, '/'+ level_3.controller_name.to_s) rescue link_to @no_route.html_safe %>
				<% end %>
			</li>
	<% end %>
	</ul>
<% end %>	
</li>